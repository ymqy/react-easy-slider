{
  "name": "react-easy-slider",
  "version": "1.0.0",
  "description": "react-easy-slider is a slider component of React",
  "main": "dist/react-easy-slider.umd.js",
  "module": "dist/react-easy-slider.esm.js",
  "browser": "dist/react-easy-slider.umd.js",
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "cross-env TIMING=1 eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*.tsx\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymqy/react-easy-slider.git"
  },
  "keywords": [
    "slider"
  ],
  "author": "MingYe <ymqyit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ymqy/react-easy-slider/issues"
  },
  "homepage": "https://github.com/ymqy/react-easy-slider#readme",
  "devDependencies": {
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/react": "^17.0.4",
    "@types/react-transition-group": "^4.4.1",
    "@umijs/fabric": "^2.5.7",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.14",
    "react": "^17.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0"
  },
  "dependencies": {
    "react-transition-group": "^4.4.1"
  }
}
